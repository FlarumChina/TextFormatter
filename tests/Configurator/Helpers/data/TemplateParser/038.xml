<?xml version="1.0"?>
<template outputMethod="html">
	<switch branch-key=".">
		<case branch-values='[":)"]' test=".=':)'">
			<output escape="text" type="literal">.</output>
		</case>
		<case branch-values='[":("]' test=".=':('">
			<output escape="text" type="literal">.</output>
		</case>
		<case branch-values='[":p"]' test=".=':p'">
			<output escape="text" type="literal">.</output>
		</case>
		<case branch-values='[";)"]' test=".=';)'">
			<output escape="text" type="literal">.</output>
		</case>
	</switch>
</template>