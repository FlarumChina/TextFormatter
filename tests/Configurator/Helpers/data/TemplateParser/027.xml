<stylesheet outputMethod="xml">
	<template>
		<match priority="0">FOO</match>
		<element name="div" id="1">
			<switch>
				<case test="@foo">
					<closeTag id="1"/>
					<output type="literal">foo</output>
				</case>
				<case test="@bar">
					<closeTag id="1"/>
					<output type="literal">bar</output>
				</case>
				<case>
					<attribute name="id">
						<output type="literal">id</output>
					</attribute>
					<closeTag id="1"/>
					<output type="literal">baz</output>
				</case>
			</switch>
		</element>
	</template>
</stylesheet>