<?xml version="1.0"?>
<template outputMethod="html">
	<switch branch-key=".">
		<case branch-values="[&quot;'&quot;]" test=".=&quot;'&quot;">
			<output escape="text" type="literal">.</output>
		</case>
		<case branch-values="[&quot;\&quot;&quot;]" test=".='&quot;'">
			<output escape="text" type="literal">.</output>
		</case>
		<case branch-values="[&quot;\&quot;'\&quot;&quot;]" test=".=concat('&quot;', &quot;'&quot;, '&quot;')">
			<output escape="text" type="literal">.</output>
		</case>
		<case branch-values='[""]' test=".=''">
			<output escape="text" type="literal">.</output>
		</case>
	</switch>
</template>