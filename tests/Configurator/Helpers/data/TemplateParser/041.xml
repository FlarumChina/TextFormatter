<?xml version="1.0"?>
<template outputMethod="html">
	<switch hash-key=".">
		<case hash-value=":)" test=".=':)'">
			<output escape="text" type="literal">.</output>
		</case>
		<case hash-value=":(" test=".=':('">
			<output escape="text" type="literal">.</output>
		</case>
		<case hash-value=":p" test=".=':p'">
			<output escape="text" type="literal">.</output>
		</case>
		<case hash-value=";)" test=".=';)'">
			<output escape="text" type="literal">.</output>
		</case>
		<case>
			<output escape="text" type="literal">X</output>
		</case>
	</switch>
</template>