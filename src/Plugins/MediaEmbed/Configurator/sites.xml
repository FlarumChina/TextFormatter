		<example>http://www.amazon.ca/gp/product/B00GQT1LNO/</example>
		<example>http://www.amazon.co.jp/gp/product/B003AKZ6I8/</example>
		<example>http://www.amazon.co.uk/gp/product/B00BET0NR6/</example>
		<example>http://www.amazon.com/dp/B002MUC0ZY</example>
		<example>http://www.amazon.com/The-BeerBelly-200-001-80-Ounce-Belly/dp/B001RB2CXY/</example>
		<example>http://www.amazon.com/gp/product/B0094H8H7I</example>
		<example>http://www.amazon.de/Netgear-WN3100RP-100PES-Repeater-integrierte-Steckdose/dp/B00ET2LTE6/</example>
		<example>http://www.amazon.fr/Vans-Authentic-Baskets-mixte-adulte/dp/B005NIKPAY/</example>
		<example>http://www.amazon.it/gp/product/B00JGOMIP6/</example>

		<host>amazon.ca</host>
		<host>amazon.co.uk</host>
		<host>amazon.co.jp</host>
		<host>amazon.com</host>
		<host>amazon.de</host>
		<host>amazon.fr</host>
		<host>amazon.it</host>
		<extract>#/(?:dp|gp/product)/(?'id'[A-Z0-9]+)#</extract>
		<extract>#amazon\.(?:co\.)?(?'tld'ca|de|fr|it|jp|uk)#</extract>
		<iframe width="120" height="240">
			<src><![CDATA[
				<xsl:text>//rcm</xsl:text>
				<xsl:if test="@tld and contains('cadefritjpuk',@tld)">-<xsl:value-of select="@tld"/></xsl:if>
				<xsl:text>.amazon.</xsl:text>
				<xsl:choose>
					<xsl:when test="@tld='jp' or @tld='uk'">co.<xsl:value-of select="@tld"/></xsl:when>
					<xsl:when test="@tld and contains('cadefrit',@tld)"><xsl:value-of select="@tld"/></xsl:when>
					<xsl:otherwise>com</xsl:otherwise>
				</xsl:choose>
				<xsl:text>/e/cm?lt1=_blank&amp;bc1=FFFFFF&amp;bg1=FFFFFF&amp;fc1=000000&amp;lc1=0000FF&amp;p=8&amp;l=as1&amp;f=ifr&amp;asins=</xsl:text>
				<xsl:value-of select="@id"/>
				<xsl:text>&amp;o=</xsl:text>
				<xsl:choose>
					<xsl:when test="@tld='ca'">15</xsl:when>
					<xsl:when test="@tld='de'">3</xsl:when>
					<xsl:when test="@tld='fr'">8</xsl:when>
					<xsl:when test="@tld='it'">29</xsl:when>
					<xsl:when test="@tld='jp'">9</xsl:when>
					<xsl:when test="@tld='uk'">2</xsl:when>
					<xsl:otherwise>1</xsl:otherwise>
				</xsl:choose>
				<xsl:text>&amp;t=</xsl:text>
				<xsl:choose>
					<xsl:when test="$AMAZON_ASSOCIATE_TAG"><xsl:value-of select="$AMAZON_ASSOCIATE_TAG"/></xsl:when>
					<xsl:otherwise>_</xsl:otherwise>
				</xsl:choose>
			]]></src>
		</iframe>
	</site>

	<site id="audiomack">
		<name>Audiomack</name>
		<homepage>http://www.audiomack.com/</homepage>
		<example>http://www.audiomack.com/song/your-music-fix/jammin-kungs-remix-1</example>
		<example>http://www.audiomack.com/album/chance-the-rapper/acid-rap</example>

		<host>audiomack.com</host>
		<extract>!audiomack\.com/(?'mode'album|song)/(?'id'[-\w]+/[-\w]+)!</extract>
		<iframe width="100%">
			<height><![CDATA[
				<xsl:choose>
					<xsl:when test="@mode='album'">352</xsl:when>
					<xsl:otherwise>144</xsl:otherwise>
				</xsl:choose>
			]]></height>
			<src><![CDATA[
				<xsl:text>//www.audiomack.com/embed3</xsl:text>
				<xsl:if test="@mode='album'">-album</xsl:if>
				<xsl:text>/</xsl:text>
