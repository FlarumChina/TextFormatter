	<host>gty.im</host>
	<extract>!gty\.im/(?'id'\d+)!</extract>
	<extract>!gettyimages\.[.\w]+/detail(?=/).*?/(?'id'\d+)!</extract>
	<scrape url="http://embed.gettyimages.com/preview/{@id}">
		<extract>!"height":[ "]*(?'height'\d+)!</extract>
		<extract>!"width":[ "]*(?'width'\d+)!</extract>
		<extract>!et=(?'et'[-=\w]+)!</extract>
		<extract>!sig=(?'sig'[-=\w]+)!</extract>
	</scrape>
	<iframe width="{@width}" height="{49+@height}" src="//embed.gettyimages.com/embed/{@id}?et={@et}&amp;similar=on&amp;sig={@sig}"/>
</site>