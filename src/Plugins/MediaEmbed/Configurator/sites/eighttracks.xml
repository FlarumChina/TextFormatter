<site name="8tracks" homepage="http://8tracks.com/">
	<example>http://8tracks.com/fingerlickengood/just-nujabes</example>
	<example>http://8tracks.com/midna/2242699</example>

	<tags><tag>music</tag></tags>

	<host>8tracks.com</host>
	<extract>!8tracks\.com/[-\w]+/(?'id'\d+)(?=#|$)!</extract>
	<scrape>
		<match>!8tracks\.com/[-\w]+/[-\w]+!</match>
		<extract>!eighttracks://mix/(?'id'\d+)!</extract>
	</scrape>
	<iframe width="400" height="400" src="//8tracks.com/mixes/{@id}/player_v3_universal"/>
</site>