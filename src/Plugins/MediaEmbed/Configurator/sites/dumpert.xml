<site name="dumpert" homepage="http://www.dumpert.nl/">
	<example>http://www.dumpert.nl/mediabase/6622577/4652b140/r_mi_gaillard_doet_halloween_prank.html</example>

	<tags><tag>.nl</tag><tag>entertainment</tag></tags>

	<host>dumpert.nl</host>
	<scrape>
		<match>!dumpert\.nl/mediabase/\d+/\w+!</match>
		<extract>!data-itemid="(?'id'\w+)!</extract>
	</scrape>
	<iframe src="http://www.dumpert.nl/embed/{translate(@id,'_','/')}/"/>
</site>