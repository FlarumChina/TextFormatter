<sites>
	<site id="collegehumor">
		<name>CollegeHumor</name>
		<homepage>http://www.collegehumor.com/</homepage>
		<example>http://www.collegehumor.com/video/1181601/more-than-friends</example>

		<host>collegehumor.com</host>
		<extract>!collegehumor\.com/(?:video|embed)/(?'id'[0-9]+)!</extract>
		<iframe width="600" height="369" src="http://www.collegehumor.com/e/{@id}"/>
	</site>

	<site id="dailymotion">
		<name>Dailymotion</name>
		<homepage>http://www.dailymotion.com/</homepage>
		<source>http://www.dailymotion.com/doc/api/player</source>
		<example>http://www.dailymotion.com/video/x222z1</example>
		<example>http://www.dailymotion.com/user/Dailymotion/2#video=x222z1</example>

		<host>dailymotion.com</host>
		<extract>!dailymotion\.com/(?:video/|user/[^#]+#video=)(?'id'[A-Za-z0-9]+)!</extract>
		<flash width="560" height="315" src="http://www.dailymotion.com/swf/{@id}"/>
	</site>

	<site id="facebook">
		<name>Facebook</name>
		<homepage>http://www.facebook.com/</homepage>
		<example>https://www.facebook.com/photo.php?v=10100658170103643&amp;set=vb.20531316728&amp;type=3&amp;theater</example>

		<host>facebook.com</host>
		<extract>!facebook\.com/photo\.php\?v=(?'id'[0-9]+)!</extract>
		<iframe width="560" height="315" src="https://www.facebook.com/video/embed?video_id={@id}"/>
	</site>

	<site id="funnyordie">
		<name>Funny or Die</name>
		<homepage>http://www.funnyordie.com/</homepage>
		<source>http://support.funnyordie.com/discussions/problems/918-embed-user-videos-widget</source>
		<example>http://www.funnyordie.com/videos/bf313bd8b4/murdock-with-keith-david</example>

		<host>funnyordie.com</host>
		<extract>!funnyordie.com/videos/(?'id'[0-9a-f]+)!</extract>
		<iframe width="640" height="360" src="http://www.funnyordie.com/embed/{@id}"/>
	</site>

	<site id="liveleak">
		<name>LiveLeak</name>
		<homepage>http://www.liveleak.com/</homepage>
		<example>http://www.liveleak.com/view?i=3dd_1366238099</example>

		<host>liveleak.com</host>
		<extract>!liveleak\.com/view\?i=(?'id'[a-f_0-9]+)!</extract>
		<iframe width="560" height="315" src="http://www.liveleak.com/e/{@id}"/>
	</site>

	<site id="metacafe">
		<name>Metacafe</name>
		<homepage>http://www.metacafe.com/</homepage>
		<example>http://www.metacafe.com/watch/10785282/chocolate_treasure_chest_epic_meal_time/</example>

		<host>metacafe.com</host>
		<extract>!metacafe\.com/watch/(?'id'[0-9]+)!</extract>
		<iframe width="560" height="315" src="http://www.metacafe.com/embed/{@id}/"/>
	</site>

	<site id="slideshare">
		<name>SlideShare</name>
		<homepage>http://www.slideshare.net/</homepage>
		<source>http://help.slideshare.com/forums/67665-Embedding-Sharing</source>
		<example>http://www.slideshare.net/Slideshare/how-23431564</example>

		<host>slideshare.net</host>
		<extract>!slideshare\.net/[^/]+/[-\w]+-(?'id'[0-9]{6,})$!</extract>
		<scrape>
			<match>!slideshare\.net/[^/]+/\w!</match>
			<extract>!"presentationId":(?'id'[0-9]+)!</extract>
		</scrape>
		<iframe width="427" height="356" src="http://www.slideshare.net/slideshow/embed_code/{@id}"/>
	</site>

	<site id="soundcloud">
		<name>SoundCloud</name>
		<homepage>https://soundcloud.com/</homepage>
		<source>https://soundcloud.com/pages/widgets</source>
		<example>http://api.soundcloud.com/tracks/98282116</example>

		<host>soundcloud.com</host>
		<extract>!soundcloud\.com(?:%2F|/)tracks(?:%2F|/)(?'id'[0-9]+)!</extract>
		<iframe width="560"
		        height="166"
		        src="https://w.soundcloud.com/player/?url=http%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F{@id}"
		/>
	</site>

	<site id="ted">
		<name>TED Talks</name>
		<homepage>http://www.ted.com/</homepage>
		<source>http://blog.ted.com/2011/04/01/now-you-can-embed-tedtalks-with-subtitles-enabled/</source>
		<example>http://www.ted.com/talks/eli_pariser_beware_online_filter_bubbles.html</example>
		<example>http://embed.ted.com/playlists/26/our_digital_lives.html</example>

		<host>ted.com</host>
		<extract>!ted\.com/(?'path'(?:talk|playlist)s/[^\s"]+)!i</extract>
		<iframe width="560" height="315" src="http://embed.ted.com/{@path}"/>
	</site>

	<site id="twitch">
		<name>Twitch</name>
		<homepage>http://www.twitch.tv/</homepage>
		<source>https://github.com/justintv/Twitch-API/blob/master/embedding.md</source>
		<example>http://www.twitch.tv/minigolf2000</example>
		<example>http://www.twitch.tv/minigolf2000/c/2475925</example>
		<example>http://www.twitch.tv/minigolf2000/b/361358487</example>

		<host>twitch.tv</host>
		<extract>!twitch\.tv/(?'channel'[A-Za-z0-9]+)(?:/b/(?'archive_id'[0-9]+)|/c/(?'chapter_id'[0-9]+))?!</extract>
		<template><![CDATA[
			<object type="application/x-shockwave-flash"
			        typemustmatch=""
			        width="620"
			        height="378"
			        data="http://www.twitch.tv/widgets/{substring('archl',5-4*boolean(@archive_id|@chapter_id),4)}ive_embed_player.swf"
			>
				<param name="flashvars">
					<xsl:attribute name="value">channel=<xsl:value-of select="@channel"/><xsl:if test="@archive_id">&amp;archive_id=<xsl:value-of select="@archive_id"/></xsl:if><xsl:if test="@chapter_id">&amp;chapter_id=<xsl:value-of select="@chapter_id"/></xsl:if></xsl:attribute>
				</param>
				<embed type="application/x-shockwave-flash"
				       width="620"
				       height="378"
				       src="http://www.twitch.tv/widgets/{substring('archl',5-4*boolean(@archive_id|@chapter_id),4)}ive_embed_player.swf"
				/>
			</object>
		]]></template>
	</site>

	<site id="vimeo">
		<name>Vimeo</name>
		<homepage>http://vimeo.com/</homepage>
		<source>http://developer.vimeo.com/player/embedding</source>
		<example>http://vimeo.com/67207222</example>
		<example>http://vimeo.com/channels/staffpicks/67207222</example>

		<host>vimeo.com</host>
		<extract>!vimeo\.com/(?:channels/[^/]+/)?(?'id'[0-9]+)!</extract>
		<iframe width="560" height="315" src="http://player.vimeo.com/video/{@id}"/>
	</site>

	<site id="youtube">
		<name>YouTube</name>
		<homepage>http://www.youtube.com/</homepage>
		<source>http://support.google.com/youtube/bin/answer.py?hl=en&amp;answer=171780</source>
		<example>http://www.youtube.com/watch?v=-cEzsCAzTak</example>
		<example>http://youtu.be/-cEzsCAzTak</example>

		<host>youtube.com</host>
		<host>youtu.be</host>
		<extract>!youtube\.com/(?:watch\?.*?v=|v/)(?'id'[-0-9A-Z_a-z]+)!</extract>
		<extract>!youtu\.be/(?'id'[-0-9A-Z_a-z]+)!</extract>
		<iframe width="560" height="315" src="http://www.youtube.com/embed/{@id}"/>
	</site>
</sites>